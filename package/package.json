{
    "name": "croct-nanostores",
    "version": "0.2.0",
    "type": "module",
    "description": "Nanostores bindings for Croct content.",
    "keywords": [
        "nanostores",
        "state-management",
        "croct",
        "personalization",
        "typescript",
        "react",
        "preact",
        "vue",
        "solid-js",
        "svelte"
    ],
    "homepage": "https://github.com/Fryuni/croct-nano",
    "bugs": {
        "url": "https://github.com/Fryuni/croct-nano/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fryuni/croct-nano.git"
    },
    "license": "MIT",
    "author": {
        "name": "Luiz Ferraz",
        "email": "luiz+lib-croct-nano@lferraz.com",
        "url": "https://github.com/Fryuni/croct-nano"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "README.md",
        "build",
        "src"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "prepublish": "pnpm run build && cp ../README.md .",
        "size": "rm -f esbuild-why*.html && pnpm size-limit",
        "test": "vitest run --coverage",
        "test:dev": "vitest --coverage.enabled=true"
    },
    "dependencies": {
        "@croct/json": "^2.1.0",
        "@croct/plug": "^0.21.0",
        "@nanostores/persistent": "^1.3.3",
        "nanostores": "^1.1.0"
    },
    "devDependencies": {
        "@size-limit/esbuild-why": "^11.1.6",
        "@size-limit/preset-small-lib": "^11.1.6",
        "@types/node": "^22.13.4",
        "@vitest/coverage-v8": "^3.0.5",
        "@vitest/ui": "^3.0.5",
        "jest-extended": "^4.0.2",
        "size-limit": "^11.1.6",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3",
        "vite": "^6.1.0",
        "vitest": "^3.0.5"
    }
}
